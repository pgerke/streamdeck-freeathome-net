<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>PhilipGerke.StreamDeckFreeAtHome</RootNamespace>
    <Authors>Philip Gerke</Authors>
    <PackageId>PhilipGerke.StreamDeckFreeAtHome</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Product>Stream Deck free@home</Product>
    <PackageProjectUrl>https://github.com/pgerke/streamdeck-freeathome</PackageProjectUrl>
    <RepositoryUrl>https://github.com/pgerke/streamdeck-freeathome</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>StreamDeck FreeAtHome</PackageTags>
    <Version>0.3.0-alpha</Version>
    <Description>A plugin for the Elgato Stream Deck that interfaces with the free@home System Access Point</Description>
    <Copyright>Philip Gerke Â© 2021</Copyright>
    <AssemblyName>com.philipgerke.freeathome</AssemblyName>
    <ApplicationIcon>pluginIcon.ico</ApplicationIcon>
    <PackageIcon>pluginIcon@2x.png</PackageIcon>
    <StartupObject>PhilipGerke.StreamDeckFreeAtHome.Program</StartupObject>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>StreamDeckFreeAtHome.xml</DocumentationFile>
    <OutputPath></OutputPath>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath></OutputPath>
  </PropertyGroup>

